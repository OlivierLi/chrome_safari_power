set myURL to "https://meet.google.com/{{ meeting_id }}"

tell application "Safari"
    if it is running then
        activate
        delay 3
        open location {myURL}
        delay 3

        tell document 1
            do JavaScript "b=document.getElementsByClassName('uArJ5e UQuaGc Y5sE8d uyXBBb xKiqt')[0];b.click();"
        end tell

        delay 1800
    end if
end tell
