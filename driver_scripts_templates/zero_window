tell application "{{ browser }}" 
    -- If {{ browser }} is already started then just bring
    -- it to the forefront otherwise open it.
    if it is running then
      reopen
    else
      activate
    end if

    set bounds of front window to {0, 0, 1280, 720}

    open location "about:blank"
    delay 10

    close every window

    delay {{ delay }} 

    activate

end tell
